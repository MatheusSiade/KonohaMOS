<h1>Listing people</h1>

<table>
  <tr>
    <td>Name</td>
    <td>User Id</td>

  </tr>

  <% @person.each do |person| %>
      <tr>
        <td><%=person.name%></mission.title></td>
        <td><%=person.user_id%></td>
        <td><%= link_to 'show',person_path(person) %></td>
        <td><%= link_to 'edit',edit_person_path(person) %> </td>
        <td><%= link_to 'destroy',person_path(person),
                method: :delete,
                data: { confirm: 'Are you sure?'} %> </td>

      </tr>
  <%end%>
</table>
<a class="waves-effect waves-light btn" href="people/new.html">New person</a>
